{
    "name": "quanta/middleware",
    "type": "library",
    "description": "Minimalist library for dispatching Psr-15 middleware",
    "keywords": [
        "psr-15",
        "dispatcher",
        "middleware",
        "request-handler"
    ],
    "license": "MIT",
    "homepage": "https://github.com/quantaphp/middleware",
    "support": {
        "issues": "https://github.com/quantaphp/middleware/issues",
        "source": "https://github.com/quantaphp/middleware"
    },
    "authors": [
        {
            "name": "pmall",
            "email": "pierre.mallinjoud@gmail.com"
        }
    ],
    "config": {
        "sort-packages": true
    },
    "require": {
        "php": ">=7.0",
        "psr/http-message": "^1.0",
        "psr/http-server-handler": "^1.0",
        "psr/http-server-middleware": "^1.0"
    },
    "require-dev": {
        "eloquent/phony-kahlan": "^2.0",
        "kahlan/kahlan": "^4.5"
    },
    "autoload": {
        "psr-4": {
            "Quanta\\Http\\": "src"
        }
    }
}
